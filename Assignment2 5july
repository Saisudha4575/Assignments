public class ArraySum {  
    public static void main(String[] args) {  
        int [] arr = new int [] {18,2,5,7,11};  
        int sum = 0;  
        for (int i = 0; i < arr.length; i++) {  
           sum = sum + arr[i];  
        }  
        System.out.println("Sum of elements in an array: " + sum);  
    }  
}  
